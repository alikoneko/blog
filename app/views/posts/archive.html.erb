<h1>
    Posts
</h1>
<table class="table table-hover" >
    <tr>
        <th>title</th>
        <th> </th>
    </tr>
    <% posts.each do |post| %>
    <tr>
        <td><%= link_to post.title, [post] %></td>
        <td><%= post.created_at.localtime.strftime("%A, %B %e, %Y at %I:%M%p") %></td>
    </tr>
    <% end %>
</table>