<div class="row clearfix">
    <div class="col-md-3 pull-right">
        <%= render 'layouts/aside' %>
    </div>
    <div class="col-md-9 pull-left">
        <% posts.each do |post| %>
            <%= render partial: 'post', locals: { post: post }%>
        <% end %>
    </div>
</div>

<%= paginate @posts, theme: 'twitter-bootstrap-3' %>
